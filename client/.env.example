## ----------------------------------------------------------------------------
## Frontend Environment Variables
## ----------------------------------------------------------------------------

## The URL of the backend API
# REACT_APP_API_URL=http://localhost:5000/api

## The name of the frontend application
# REACT_APP_NAME=HappiHub

## Enable or disable debugging
# REACT_APP_DEBUG=true

## Google Analytics ID for tracking
# REACT_APP_GOOGLE_ANALYTICS_ID=UA-XXXXXX-X

## ----------------------------------------------------------------------------
## Authentication and Security
## ----------------------------------------------------------------------------

## OAuth client ID for third-party authentication
# REACT_APP_OAUTH_CLIENT_ID=your_oauth_client_id

## OAuth client secret for third-party authentication
# REACT_APP_OAUTH_CLIENT_SECRET=your_oauth_client_secret

## ----------------------------------------------------------------------------
## Third-Party APIs
## ----------------------------------------------------------------------------

## API key for a third-party service
# REACT_APP_THIRD_PARTY_API_KEY=your_api_key

## API secret for a third-party service
# REACT_APP_THIRD_PARTY_API_SECRET=your_api_secret

## ----------------------------------------------------------------------------
## Logging and Monitoring
## ----------------------------------------------------------------------------

## Log level for frontend logging
# REACT_APP_LOG_LEVEL=info
# REACT_APP_LOG_LEVEL=debug
# REACT_APP_LOG_LEVEL=error
# REACT_APP_LOG_LEVEL=warn
# REACT_APP_LOG_LEVEL=trace
# REACT_APP_LOG_LEVEL=silent
# REACT_APP_LOG_LEVEL=verbose

## Monitoring service URL (e.g., for services like Datadog, New Relic)
# REACT_APP_MONITORING_SERVICE_URL=https://monitoring-service.com

## ----------------------------------------------------------------------------
## Payment Gateway Configuration
## ----------------------------------------------------------------------------

## Payment gateway public key (e.g., Stripe, PayPal)
# REACT_APP_PAYMENT_GATEWAY_PUBLIC_KEY=your_payment_gateway_public_key

## ----------------------------------------------------------------------------
## Miscellaneous
## ----------------------------------------------------------------------------

## Feature flag for enabling/disabling features
# REACT_APP_FEATURE_FLAG_NEW_FEATURE=true

## Maximum upload file size (e.g., 10MB)
# REACT_APP_MAX_UPLOAD_SIZE=10MB
