# General Application Settings
# REACT_APP_ENV=development
# REACT_APP_PORT=3000
# REACT_APP_NAME=HappiHub

# API Configuration
# REACT_APP_API_BASE_URL=http://localhost:5000/api

# Authentication Configuration
# REACT_APP_AUTH_DOMAIN=your_auth_domain
# REACT_APP_AUTH_CLIENT_ID=your_auth_client_id
# REACT_APP_AUTH_REDIRECT_URI=http://localhost:3000/callback

# Third-Party Service Configuration (Optional)
# REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
# REACT_APP_STRIPE_PUBLIC_KEY=your_stripe_public_key

# Feature Flags (Optional)
# REACT_APP_FEATURE_X_ENABLED=true
# REACT_APP_FEATURE_Y_ENABLED=false

# Analytics Configuration (Optional)
# REACT_APP_GOOGLE_ANALYTICS_ID=your_google_analytics_id
# REACT_APP_SENTRY_DSN=your_sentry_dsn

# Social Media Integration (Optional)
# REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id
# REACT_APP_TWITTER_API_KEY=your_twitter_api_key
# REACT_APP_LINKEDIN_CLIENT_ID=your_linkedin_client_id

# CORS Configuration
# REACT_APP_CORS_ALLOWED_ORIGINS=http://localhost:5000

# Debugging and Development Tools
# REACT_APP_DEBUG=true

# Additional Production Configuration
# Uncomment and set appropriate values for production
# REACT_APP_ENV=production
# REACT_APP_API_BASE_URL=https://your-production-api-url.com/api
# REACT_APP_AUTH_REDIRECT_URI=https://your-production-client-url.com/callback
# REACT_APP_CORS_ALLOWED_ORIGINS=https://your-production-api-url.com
